{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.min.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "src": "/(.*)",
      "dest": "dist/server.min.js"
    }
  ],
  "env":{
    "NODE_ENV":"production"
  },
  "headers": [
    {
      "source": "/(.*).(js|css|png|jpg|jpeg|gif|ico|svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}